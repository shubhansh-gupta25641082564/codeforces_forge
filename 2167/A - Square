import java.io.*;import java.util.*; public class Main {    static final InputStream in = System.in;    static final byte[] buf = new byte[1 << 16];    static int p = 0, n = 0;     static int read() throws IOException {        if (p >= n) {            n = in.read(buf);            p = 0;            if (n < 0) return -1;        }        return buf[p++];    }    static int nextInt() throws IOException {        int c, s = 1, x = 0;        do { c = read(); } while (c <= 32);        if (c == '-') { s = -1; c = read(); }        while (c > 32) { x = x * 10 + c - '0'; c = read(); }        return x * s;    }     public static void main(String[] args) throws Exception {        StringBuilder out = new StringBuilder();        int t = nextInt();        int[] v = new int[4];        while (t-- > 0) {            v[0] = nextInt();            v[1] = nextInt();            v[2] = nextInt();            v[3] = nextInt();            Arrays.sort(v);            out.append(v[0] == v[3] ? "YES" : "NO").append('\n');        }        System.out.print(out.toString());    }}